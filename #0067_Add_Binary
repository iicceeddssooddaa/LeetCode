class Solution(object):
    def addBinary(self, a, b):
        """
        :type a: str
        :type b: str
        :rtype: str
        """
        a_int = int(a,2)
        b_int = int(b,2)
        result = "{0:b}".format(a_int + b_int)
        return (result)
        #快于84%,少于78%
        #并不是每个case都这模样
        
        #result = "{0:b}".format(int(a,2) + int(b,2))
